/* components/tu-popout/index.wxss */
.choosebox{
  width: calc(100% - 48rpx);
  padding: 0 24rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9999;
}
.choosebox .choosedetail{
  width: 100%;
  height: auto;
  border-radius: 10rpx;
  overflow: hidden;
}
.choosebox .choosedetail .title{
  width: 100%;
  height: 70rpx;
  background: #fff;
  font-size: 26rpx;
  color: #999999;
  text-align: center;
  line-height: 70rpx;
  border-bottom: 1rpx solid #CCCCCC;
}
.choosebox .choosedetail .choice{
  width: 100%;
  height: 98rpx;
  background: #fff;
  text-align: center;
  line-height: 98rpx;
  font-size: 36rpx;
  color: #129EEF;
  border-bottom: 1rpx solid #CCCCCC;
}
.choosebox .choosedetail .choice:last-child{
  border-bottom: none;
}
.choosebox .cancel{
  width: 100%;
  height: 96rpx;
  background: #fff;
  text-align: center;
  line-height: 96rpx;
  font-size: 36rpx;
  color: #129EEF;
  border-radius: 10rpx;
  margin: 12rpx 0;
}